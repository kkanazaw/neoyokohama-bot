{
  "name": "neoyokohama-bot",
  "version": "0.0.1",
  "description": "neoyokohama bot",
  "engines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "node run.js",
    "mocha": "mocha test/test_*.es6",
    "eslint": "eslint *.es6 src/*.es6 test/*.es6",
    "test": "npm run eslint && npm run mocha",
    "build": "babel src/ --out-dir dist/ --source-maps inline",
    "watch": "parallelshell 'npm run build -- --watch' 'chokidar \"(src|test)/*.es6\" -c \"npm run eslint\"'",
    "zip": "rm bundle.zip; zip -r bundle.zip index.js .env dist/ node_modules/"
  },
  "keywords": [
    "bot"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.3.17",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "cheerio": "^0.20.0",
    "co": "^4.6.0",
    "date-utils": "^1.2.17",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "twitter": "^1.2.5",
    "weather-yahoo-jp": "^0.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^2.2.0",
    "eslint-plugin-if-in-test": "^0.2.0",
    "mocha": "^2.3.4",
    "parallelshell": "^2.0.0"
  }
}
