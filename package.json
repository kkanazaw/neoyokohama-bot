{
  "name": "neoyokohama-bot",
  "version": "0.0.1",
  "description": "neoyokohama bot",
  "engines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "node run.js",
    "mocha": "mocha 'test/**.js'",
    "eslint": "eslint '*.js' '{src,test}/{*,**}.js'",
    "test": "npm run mocha && npm run eslint",
    "build": "babel src/ --out-dir dist/ --source-maps inline",
    "watch": "parallelshell 'npm run build -- --watch' 'chokidar \"(src|test)/*.js\" -c \"npm run eslint\"'",
    "zip": "rm bundle.zip; zip -r bundle.zip index.js .env dist/ node_modules/",
    "clean": "rm -rf dist/"
  },
  "keywords": [
    "bot"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "cheerio": "^0.20.0",
    "date-utils": "^1.2.19",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "twitter": "^1.2.5",
    "weather-yahoo-jp": "^0.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^2.7.0",
    "eslint-plugin-if-in-test": "^0.2.0",
    "mocha": "^2.4.5",
    "parallelshell": "^2.0.0"
  }
}
