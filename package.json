{
  "name": "neoyokohama-bot",
  "version": "0.0.1",
  "description": "neoyokohama bot",
  "engines": {
    "node": ">= 5.2.0"
  },
  "main": "index.js",
  "scripts": {
    "mocha": "mocha test/test_*.es6 --compilers js:babel-register",
    "eslint": "eslint *.es6 src/*.es6 test/*.es6",
    "test": "npm run eslint && npm run mocha",
    "watch": "parallelshell 'npm run build -- --watch' 'esw *.es6 src/*.es6 --watch'",
    "start": "node index.js",
    "build": "babel src/ --out-dir dist/ --source-maps inline",
    "zip": "zip -r bundle.zip index.js .env dist/ node_modules/"
  },
  "keywords": [
    "bot"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "cheerio": "^0.19.0",
    "co": "^4.6.0",
    "date-utils": "^1.2.17",
    "debug": "^2.2.0",
    "dotenv": "^1.2.0",
    "lodash": "^3.10.1",
    "superagent": "^1.6.1",
    "twitter": "^1.2.5"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "eslint": "^1.10.3",
    "eslint-watch": "^2.1.4",
    "mocha": "^2.3.4",
    "parallelshell": "^2.0.0"
  }
}
